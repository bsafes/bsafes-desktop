<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>Sign In</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <%
      var fontawesomeV4 = true;
      var isDemo = true;
    %>
    <% include stylesheets %>
    <% include commonHeader %>
  </head>

  <body class="">
    <% include navBarWithoutSignIn %>
    <div>
      <div class="col-xs-10">
        <ul class="breadcrumb">
          <li><a id="" href="#">Member Sign In</a></li>
        </ul>
      </div>
    </div>
    <div class="container">
      <div class="row">
        <div class="col-xs-12 col-xs-offset-0 col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2 textAlignCenter">
          <h2>Member Sign In</h2>
        </div>
      </div>
      <%if(typeof(isDemo)!== "undefined" && isDemo){%>
        <div class="demoMember row">
          <div class="col-xs-12 col-xs-offset-0 col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2">
            <p>Demo Member Name: <span class="text-primary">Demo</span>, Password: <span class="text-primary">4Ur#Only</span></p>
          </div>
        </div>
      <%} else {%>
        <br>
      <%}%>
      <div class="row managedMemberSignInForm signInForm">
        <div class="col-xs-12 col-xs-offset-0 col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2">
          <form class="form-horizontal backgroundColorWhite">
            <fieldset>
              <br>
              <div class="row form-group">
                <div class="col-xs-10 col-xs-offset-1 col-sm-offset-3 col-sm-6">
                  <label for="masterId" class="control-label">Account ID</label>
                  <span class="hidden" id="masterId"><%=masterId%></span>
                  <h4 id="displayMasterId"><%=displayMasterId%></h4>
                </div>
              </div>
              <div class="row form-group">
                <div class="col-xs-10 col-xs-offset-1 col-sm-offset-3 col-sm-6">
                  <label for="memberName" class="control-label">Member Name</label>
                  <input type="text" class="form-control" id="memberName" placeholder="" value="Demo">
                </div>
              </div>
              <div class="row form-group">
                <div class="col-xs-10 col-xs-offset-1 col-sm-offset-3 col-sm-6">
                  <label for="password" class="control-label">Password</label>
                  <input type="password" class="form-control" id="password" placeholder=""  value="4Ur#Only">
                </div>
              </div>
              <div class="row form-group">
                <div class="col-xs-10 col-xs-offset-1 col-sm-offset-3 col-sm-6">
                  <a href="" class="btn btn-primary pull-right" id="sendBtn">Done</a>
                </div>
              </div>
              <div class="row form-group">
                <div class="col-xs-10 col-xs-offset-1 col-sm-offset-3 col-sm-6">
                  <p class="hidden warningMessage" id="invalidMember">Invalid member !</p>
                  <p class="hidden warningMessage" id="wrongPassword">Wrong passwrd !</p>
                </div>
              </div>
            </fieldset>
          </form>
        </div>
      </div>
      <br>
    </div>
    <% include navBarBottom %>
    <%
      var loadForgeJS = true;
      var managedMemberSignInJS = true;
      var signedOutState = true;
    %>
    <% include scripts %>
  </body>
</html>
