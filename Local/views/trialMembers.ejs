<!DOCTYPE html>
<html lang="en">
  <head>
		<meta charset="utf-8">
		<title>Trial Members</title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <%
      var fontawesomeV5 = true;
    %>
    <% include stylesheets %>
    <% include commonHeader %>
  </head>
  <body class="">
    <% include navBar %>
    <div class="container">
			<div class="row">
        <div class="col-xs-12 col-xs-offset-0 col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2">
					<div class="form-group">
  					<div class="input-group">
    					<input type="text" placeholder="Last 4 letters of trial ID" class="form-control input-lg" id="searchInput">
    					<span class="input-group-btn">
      					<button class="btn btn-link" type="button" id="searchBtn"><i class="fa fa-search" aria-hidden="true"></i></button>
    					</span>
  					</div>
					</div>	
				</div>
      </div>
			<div class="results">
				<div class="resultItems">
				</div>
				<div class="row moreRow hidden">
					<div class="col-xs-12 col-xs-offset-0 text-center">
						<a href="#" id="moreBtn">More</a>
					</div>
				</div>
				<br>
				<br>
				<div class="resultItemTemplate row hidden">
					<div class="col-xs-12 col-xs-offset-0 col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2">
							<div class="resultItemPanel panel panel-default">
								<div class="panel-body paddingTopBottom0Px">
									<div class="row">
										<a href="#" class="itemLink blackText">
											<div class="col-xs-12 col-xs-offset-0 marginTop7Px">
                        <h3><i class="fa fa-lock" aria-hidden="true"></i>  <span class="lastFour">AB12</span></h3>
                      </div>
											<div class="col-xs-12 col-xs-offset-0 marginTop7Px">	
												<p class="itemTitle blackText fontSize90Percent"></p>
											</div>
										</a>
									</div>
								</div>
							</div>
						</div>
				</div>
			</div>
		</div>
		<% include itemsToolbar %>
    <%
       var trialMembersJS = true;
    %>
    <% include scripts %>
  </body>
</html>
