<!DOCTYPE html>
<html lang="en">
  <head>
		<meta charset="utf-8">
		<title>Activities</title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <%
      var fontawesomeV4 = true;
    %>
    <% include stylesheets %>
    <% include commonHeader %>
  </head>
  <body class="">
		<span class='hidden' id='space'><%=space%></span>
    <% include navBar %>
    <div>
			<div class="col-xs-10">
	      <ul class="breadcrumb">
  	      <li><a id="teamName" href=""></a></li>
    	  </ul>
			</div>
    </div>
    <div class="container">
    	<div class="row">
       	<div class="col-xs-12 col-xs-offset-0 col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2 text-center">
						<h2 class="">Activities</h2>
				</div>
			</div>

			<div class="results">
				<div class="resultItems list-group">
				</div>
				<div class="row">
        	<div class="col-xs-12 col-xs-offset-0 col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2 text-center">
						<a href="#" id="moreItems">More</a>
					</div>
      	</div>
        <a href="#" class="list-group-item resultItemTemplate hidden">
          <div class="row">
            <div class="col-xs-12">
              <h4 class="itemTitle"></h4>
            </div>
					</div>
          <div class="row itemVersionRow">
            <div class="col-xs-6">
              <p class="itemVersionUpdatedBy"></p>
            </div>
            <div class="col-xs-6">
              <p class="itemVersionUpdatedTime pull-right"></p>
            </div>
          </div>
          <div class="row itemVersionRow">
						<div class="col-xs-6">
              <p class="itemVersionUpdate"></p>
            </div>
            <div class="col-xs-6">
              <p class="itemVersionUpdatedTimeStamp pull-right"></p>
            </div>
          </div>
        </a>
			</div>
			<br>
		</div>
    <%
      var loadForgeJS = true;
      var activitiesJS = true;
      var keyVerifiedState = true;
    %>
    <% include scripts %>
  </body>
</html>
